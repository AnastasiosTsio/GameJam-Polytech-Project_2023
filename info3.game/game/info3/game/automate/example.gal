Player(Init){
  * (Init)
  | Key(z) ? Move(N)  :(Init)
  | Key(d) ? Move(E)  :(Init)
  | Key(q) ? Move(W)  :(Init)
  | Key(s) ? Move(S)  :(Init)
  | True() ?          :(Init)
}

Player(Init){
  * (Init)
  | True() ?   :(Init)
}

MovingPlatform(Init){
  * (Init)
  | GotPower() ? Move() :(Init)
  | !GotPower() ? Turn() :(Init)
}