Player(Init){
  * (Init)
  | Key(f) ? Pop()    :(Init)
  | Key(z) ? Move(N)  :(Init)
  | Key(d) ? Move(E)  :(Init)
  | Key(q) ? Move(W)  :(Init)
  | Key(s) ? Move(S)  :(Init)
  | True() ?          :(Init)
}

Player(Init){
  * (Init)
  | True() ?   :(Init)
}

PowerUpBlock(Init){
  * (Init)
  | True ? :(_)
  * (Speed)
  | GotPower() ? Egg() :(Init)
  * (Shield)
  | GotPower() ? Egg() :(Init)
  * (Ammo)
  | GotPower() ? Egg() :(Init)
  * (Pow)
  | GotPower() ? Egg() :(Init)
}
