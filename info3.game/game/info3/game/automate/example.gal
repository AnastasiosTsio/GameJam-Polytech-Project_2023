Player1(Init){
  * (Init)
  | Cell(S, P) ? Pick() :(Init)
  | Key(w) & GotPower() ? Wizz():(Init)
  | Key(q) & Key(z) ? Move(NW) :(Init)
  | Key(z) & Key(d) ? Move(NE) :(Init)
  | Key(z) ? Move(N) :(Init)
  | Key(d) ? Move(E)  :(Init)
  | Key(q) ? Move(W)  :(Init)
  | Key(s) ? Move(S)  :(Init)
  | Key(a) & GotStuff() ? Pop() :(Init)
  | Key(x) ?          :(Dash)
  | True() ?          :(Init)
}

Player2(Init){
  * (Init)
  | Cell(S, P) ? Pick() :(Init)
  | Key(p) & GotPower() ? Wizz():(Init)
  | Key(b) & GotStuff() ? Pop() :(Init)
  | Key(FU) & Key(FL) ? Move(NW) :(Init)
  | Key(FU) & Key(FR) ? Move(NE) :(Init)
  | Key(FU) ? Move(N)  :(Init)
  | Key(FR) ? Move(E)  :(Init)
  | Key(FL) ? Move(W)  :(Init)
  | Key(FD) ? Move(S)  :(Init)
  | True() ?          :(Init)
}

Turret(Init){
  *(Init)
  |!GotPower? :()
  |Cell(H,A)? :(InRange)
  |True()?:(Init)

  *(InRange)
  |MyDir(N)? Wizz(N):(Init)
  |MyDir(W)? Wizz(W):(Init)
  |MyDir(E)? Wizz(E):(Init)
  |MyDir(NE)? Wizz(NE):(Init)
  |MyDir(NW)? Wizz(NW):(Init)
  |True() ? :(Init)
}

Weapon1(Init){
  * (Init)
  | Key(f) ? Pop()    :(Init)
  | Key(r) ? Wizz()    :(Init)
  | True() ?   :(Init)
}

Weapon2(Init){
  * (Init)
  | Key(m) ? Pop()    :(Init)
  | Key(n) ? Wizz()    :(Init)
  | True() ?   :(Init)
}

Bazooka(Init){
  * (Init)
  | Key(b) ? Pop() :(Dead)
  | True() ?   :(Init)

  * (Dead)
  | True() ? :()
}

Bullet(Init){
    * (Init)
  | True() ? Move()  :(Init)
}

MovingPlatform(Init){
  * (Init)
  | GotPower() ? Move() :(Init)
  | !GotPower() ? Turn() :(Init)
}

PowerUpBlock(Init){
  * (Init)
  | True() ? :(_)

  * (Speed)
  | GotPower() ? Egg() :(Init)
  | True()? :(Init)


  * (Shield)
  | GotPower() ? Egg() :(Init)
  | True() ? :(Init)

  * (Ammo)
  | GotPower() ? Egg() :(Init)
  | True() ? :(Init)

  * (Pow)
  | GotPower() ? Egg() :(Init)
  | True() ? :(Init)
}

MalusBlock(Init){
  * (Init)
  | True() ? :(_)

  * (Speed)
  | GotPower() ? Egg() :(Init)
  | True()? :(Init)

  * (Shield)
  | GotPower() ? Egg() :(Init)
  | True() ? :(Init)

  * (Ammo)
  | GotPower() ? Egg() :(Init)
  | True() ? :(Init)

  * (Pow)
  | GotPower() ? Egg() :(Init)
  | True() ? :(Init)
}


Raptor(Init){
  *(Init)
  |!GotPower ?  :()
  |MyDir(E) / MyDir(NE) / MyDir(SE) / MyDir(S) /MyDir(N)?  :(Right)
  |MyDir(W) / MyDir(NW) / MyDir(SW)?  :(Left)

  *(Right)
  |Cell(E,A) ? Pop()    :()
  |!Cell(E,O)?         :(Left)
  |!GotPower ?  :()
  |True()    ? Move(E) :(Right)

  *(Left)
  |Cell(W,A) ? Pop()    :()
  |!Cell(W,O)?          :(Right)
  |!GotPower ?          :()
  |True()    ? Move(W)  :(Left)
}